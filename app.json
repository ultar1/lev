{
  "name": "simple-app",
  "description": "A simple Node.js app with Git buildpack",
  "env": {
    "ALWAYS_ONLINE": {
      "description": "Show bot number last seen as online",
      "required": false,
      "value": "false"
    },
    "LOG_MSG": {
      "description": "Show WhatsApp messages in log",
      "required": false,
      "value": "false"
    },
    "SUDO": {
      "description": "Admin",
      "required": false,
      "value": ""
    },
    "PREFIX": {
      "description": "Prefix, check for more",
      "required": false,
      "value": "."
    },
    "SESSION_ID": {
      "description": "Session ID",
      "required": true,
      "value": ""
    },
    "STICKER_PACKNAME": {
      "description": "Sticker pack info",
      "required": false,
      "value": "ùñÄùñëùñô-ùï¨ùïΩ"
    },
    "RMBG_KEY": {
      "description": "API key from remove.bg, required for plugin rmbg",
      "required": false,
      "value": "null"
    },
    "LANGUAGE": {
      "description": "Default language for TTS or TRT",
      "required": false,
      "value": "en"
    },
    "WARN_LIMIT": {
      "description": "Maximum number of warnings to a person.",
      "required": false,
      "value": "3"
    },
    "FORCE_LOGOUT": {
      "description": "In some scenarios, have to logout web. Only in such cases, set this to true and back to false after logout.",
      "required": false,
      "value": "false"
    },
    "DISABLE_BOT": {
      "description": "Example: jid,jid,... where bot does not work in both ways.",
      "required": false,
      "value": "null"
    },
    "ANTILINK_MSG": {
      "description": "Message sent when antilink and action is kick.",
      "required": false,
      "value": "_Antilink Detected &mention kicked_"
    },
    "ANTISPAM_MSG": {
      "description": "Message sent when antispam.",
      "required": false,
      "value": "_Antispam Detected &mention kicked_"
    },
    "ANTIWORDS_MSG": {
      "description": "Message sent when antiword.",
      "required": false,
      "value": "_Antiword Detected &mention kicked_"
    },
    "ANTIWORDS": {
      "description": "Words that are not allowed in chats.",
      "required": false,
      "value": "word1,word2,word3"
    },
    "SS_TOKEN": {
      "description": "Token from https://app.screenshotapi.net/dashboard",
      "required": false,
      "value": ""
    },
    "REJECT_CALL": {
      "description": "Auto reject call, if set to true",
      "required": false,
      "value": "false"
    },
    "AUTO_STATUS_VIEW": {
      "description": "View others' status",
      "required": false,
      "value": "false"
    },
    "SEND_READ": {
      "description": "Send blue tick",
      "required": false,
      "value": "false"
    },
    "AJOIN": {
      "description": "Accept group join requests (group privacy)",
      "required": false,
      "value": "true"
    },
    "PERSONAL_MESSAGE": {
      "description": "Send a welcome message on personal chat",
      "required": false,
      "value": "null"
    },
    "DISABLE_START_MESSAGE": {
      "description": "Disable start message",
      "required": false,
      "value": "false"
    },
    "AUTO_STATUS_EMOJI": {
      "description": "Emoji for auto status updates",
      "required": false,
      "value": ""
    }
  },
  "addons": [
    {
      "plan": "heroku-postgresql"
    }
  ],
  "buildpacks": [
    {
      "url": "heroku/nodejs"
    },
    {
      "url": "https://github.com/heroku/heroku-buildpack-apt"
    }
  ],
  "formation": {
    "web": {
      "quantity": 1,
      "size": "basic"
    }
  }
}
